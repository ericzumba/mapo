{
  "name": "Mapo",
  "description": "Solr map visualization tool",
  "manifest_version": 2,
  "version": "1.0",
  "minimum_chrome_version": "23",
  "app": {
    "background": {
      "scripts": ["main.js"]
    }
  },
  "icons": {
    "16": "img/icon.png",
    "64": "img/icon.png",
    "128": "img/icon.png"
  },
  "permissions": [
    "<all_urls>"
  ]
}
