{
  "name": "Mapo",
  "description": "Solr map visualization tool",
  "manifest_version": 2,
  "version": "1.0",
  "minimum_chrome_version": "23",
  "browser_action": {
    "default_icon": "img/icon.png",
    "default_popup": "mapo.html"
  },
  "permissions": [
    "tabs",
    "activeTab",
    "webRequest",
    "webRequestBlocking",
    "<all_urls>"
  ]
}
